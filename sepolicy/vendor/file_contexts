# Audio
/mnt/vendor/persist/audio(/.*)? 									u:object_r:mtk_audiohal_data_file:s0

# Fingerprint
/data/vendor/focaltech(/.*)?           									u:object_r:vendor_fingerprint_data_file:s0
/data/vendor/fpc(/.*)?           									u:object_r:vendor_fingerprint_data_file:s0

# Lights
/(vendor|system/vendor)/bin/hw/android\.hardware\.light-service\.blossom                          	u:object_r:hal_light_default_exec:s0

# WiFi
/(vendor|system/vendor)/bin/hw/android\.hardware\.wifi@[0-9]\.[0-9]-service-lazy\.blossom 		u:object_r:hal_wifi_default_exec:s0
