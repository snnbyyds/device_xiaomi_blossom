# Audio
/mnt/vendor/persist/audio(/.*)? 									u:object_r:mtk_audiohal_data_file:s0

# Fingerprint
/data/vendor/focaltech(/.*)?           									u:object_r:vendor_fingerprint_data_file:s0
/data/vendor/fpc(/.*)?           									u:object_r:vendor_fingerprint_data_file:s0

/(vendor|system/vendor)/bin/hw/vendor\.focaltech\.fingerprint@1\.0-service		u:object_r:hal_fingerprint_default_exec:s0

/dev/focaltech_fp                                                             u:object_r:fingerprint_device:s0

# Camera
/dev/camera-dpe                                                                                         u:object_r:camera_device:s0
/dev/camera-fdvt                                                                                        u:object_r:camera_device:s0
/dev/camera-isp                                                                                         u:object_r:camera_device:s0
/mnt/vendor/persist/camera(/.*)?                                                                        u:object_r:persist_camera_data_file:s0

# Data files
/data/per_boot(/.*)?                                                 u:object_r:per_boot_file:s0

# Firmware
/vendor/firmware(/.*)?                                                                       u:object_r:vendor_firmware_file:s0

# Lights
/(vendor|system/vendor)/bin/hw/android\.hardware\.light-service\.blossom                          	u:object_r:hal_light_default_exec:s0

# WiFi
/(vendor|system/vendor)/bin/hw/android\.hardware\.wifi@[0-9]\.[0-9]-service-lazy\.blossom 		u:object_r:hal_wifi_default_exec:s0

# GPS
/dev/socket/bip_socket u:object_r:bip_socket:s0
